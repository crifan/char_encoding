    xml:id="glossary">
<title>缩略词</title>

<glossentry id="ASCII"><glossterm>ASCII</glossterm>
    <acronym>ASCII</acronym>
    <glossdef>
      <para>American Standard Code for Information Interchange</para>
      <para>美国信息交换标准代码</para>
    </glossdef>
</glossentry>

<glossentry id="BMP"><glossterm>BMP</glossterm>
    <acronym>BMP</acronym>
